.tabbar {
  margin: 30rpx 0;
  display: flex;
  justify-content: space-around;
  align-items: center;
}
.tabbar .tab {
  width: 120rpx;
  height: 50rpx;
  border: 1rpx solid #000;
  border-radius: 10rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 24rpx;
  color: #219d9c;
  box-sizing: border-box;
}
.tabbar .tab:last-child {
  width: 150rpx;
}
.tabbar .tab .picker {
  width: 100%;
  height: 100%;
  font-size: 18rpx;
}
.week {
  display: flex;
  border: 20rpx solid #00cccc;
  font-size: 22rpx;
  color: #219d9c;
}
.week-left {
  flex: 2;
}
.week-left-item {
  height: 50rpx;
  border: 1rpx solid #a6a6a6;
  border-top: 0rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: center;
  align-items: center;
}
.week-left-item:nth-child(1) {
  border-top: 1rpx solid #a6a6a6;
}
.week-left-item .mytext {
  width: 100%;
  height: 100%;
  text-align: center;
}
.week-center {
  width: 20rpx;
  background: #00cccc;
}
.week-right {
  flex: 3;
}
.week-right-title {
  display: flex;
  height: 100rpx;
  border: 1rpx solid #a6a6a6;
}
.week-right-title .left {
  width: 50rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.week-right-title .right {
  width: 60rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.week-right-title .center {
  flex: 1;
  border-left: 1rpx solid #a6a6a6;
  border-right: 1rpx solid #a6a6a6;
  font-size: 15rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.week-right-title .center .icon {
  font-size: 22rpx;
}
.week-right-content {
  display: flex;
  height: 50rpx;
  border: 1rpx solid #a6a6a6;
  border-top: 0rpx;
  box-sizing: border-box;
}
.week-right-content .left {
  width: 50rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.week-right-content .right {
  width: 60rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.week-right-content .center {
  flex: 1;
  border-left: 1rpx solid #a6a6a6;
  border-right: 1rpx solid #a6a6a6;
  display: flex;
  justify-content: center;
  align-items: center;
}
.week-right-content .center .mytext {
  width: 100%;
  height: 100%;
  text-align: center;
}
.footer {
  display: flex;
  border: 20rpx solid #00cccc;
  border-top: 0rpx;
  color: #219d9c;
}
.footer-left {
  flex: 2;
  border: 1rpx solid #a6a6a6;
}
.footer-left image {
  width: 100%;
}
.footer-center {
  width: 20rpx;
  background: #00cccc;
}
.footer-right {
  flex: 3;
  font-size: 18rpx;
}
.footer-right-title {
  display: flex;
  height: 110rpx;
  border: 1rpx solid #a6a6a6;
}
.footer-right-title .title-left {
  flex: 1.1;
  border-right: 1rpx solid #a6a6a6;
  display: flex;
  justify-content: center;
  align-items: center;
}
.footer-right-title .title-right {
  flex: 3;
  display: flex;
  justify-content: center;
  align-items: center;
}
.footer-right-content {
  display: flex;
  height: 50rpx;
}
.footer-right-content .content-left {
  flex: 1.1;
  border: 1rpx solid #a6a6a6;
  border-top: 0rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.footer-right-content .content-center,
.footer-right-content .content-right {
  flex: 1.5;
  border: 1rpx solid #a6a6a6;
  border-top: 0rpx;
  border-left: 0rpx;
}
.footer-right-content .content-center .mytext,
.footer-right-content .content-right .mytext {
  width: 100%;
  height: 100%;
  text-align: cneter;
}
.footer-url {
  display: flex;
  background: #00cccc;
  padding-bottom: 20rpx;
  justify-content: space-around;
  align-items: center;
}
